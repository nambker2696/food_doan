
<%= simple_form_for(@booking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :guess_id %>
    <%= f.input :place %>
    <%= f.input :status %>
    <%= f.input :date_booked %>
    <%= f.input :date_receipt %>
    <%= f.input :action %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
